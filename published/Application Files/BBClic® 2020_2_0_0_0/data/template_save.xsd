    <xs:element name="checkBoxMotorisationFt" type="xs:boolean"/>

    <xs:element name="fenetre_toit">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="comboBoxChoixPMft"/>
				<xs:element ref="comboBoxModeGestionFT"/>
